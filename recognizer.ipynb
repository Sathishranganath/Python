    "import speech_recognition as sr\n",
    "\n",
    "r = sr.Recognizer()\n",
    "\n",
    "with sr.Microphone() as source:\n",
    "    print(\"say something\")\n",
    "    audio = r.listen(source)\n",
    "    \n",
    "    try:\n",
    "        print(\"system predicts..\"+r.recognize_google(audio))\n",
    "    except:\n",
    "        print(\"sorry couldn't recognize\")\n",
    "        \n"
   ]
